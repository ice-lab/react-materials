  "description": "微前端方案 icestark 的子应用模板，用于快速初始化微应用",
  "homepage": "https://icestark-react.surge.sh/seller",
  "files": [
    "src/",
    "build/",
    "public/",
    "mock/",
    "tests/",
    "_gitignore",
    "_editorconfig",
    "_eslintignore",
    "_eslintrc.js",
    "_prettierignore",
    "_prettierrc.js",
    "_stylelintignore",
    "_stylelintrc.js",
    "build.json",
    "screenshot.png",
    "jsconfig.json",
    "tsconfig.json"
  ],
  "dependencies": {
    "@alifd/next": "^1.x",
    "@alifd/theme-design-pro": "^0.x",
    "@icedesign/container": "^1.0.4",
    "moment": "^2.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "build-plugin-fusion": "^0.1.0",
    "build-plugin-icestark": "^2.0.0",
    "build-plugin-moment-locales": "^0.1.0",
    "eslint": "^7.30.0",
    "ice.js": "^2.0.0",
    "stylelint": "^13.7.2"
  },
  "scripts": {
    "start": "icejs start",
    "build": "icejs build",
    "prepublishOnly": "npm run build",
    "lint": "npm run eslint && npm run stylelint",
