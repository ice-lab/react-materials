    "axios": "^0.17.1",
    "moment": "^2.24.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^5.0.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@ice/spec": "^0.1.1",
    "eslint": "^6.0.1",
    "ice-plugin-fusion": "^0.1.4",
    "ice-plugin-moment-locales": "^0.1.0",
    "ice-scripts": "^2.0.0",
    "stylelint": "^10.1.0"
