  ],
  "dependencies": {
    "@alifd/next": "^1.x",
    "@antv/data-set": "^0.9.6",
    "@icedesign/container": "^1.x",
    "@icedesign/img": "^1.x",
    "@icedesign/layout": "^1.x",
    "@icedesign/theme": "^1.x",
    "bizcharts": "^3.2.4",
    "classnames": "^2.2.5",
    "foundation-symbol": "^0.1.4",
    "moment": "^2.23.0",
    "prop-types": "^15.5.8",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@ice/spec": "^0.1.1",
    "eslint": "^6.0.1",
    "stylelint": "^10.1.0",
    "ice-scripts": "^2.0.0",
    "ice-plugin-fusion": "^0.1.4",
    "ice-plugin-moment-locales": "^0.1.0"
  },
  "scripts": {
