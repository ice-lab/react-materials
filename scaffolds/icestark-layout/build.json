{
  "vite": true,
  "publicPath": "./",
  "plugins": [
    ["build-plugin-icestark", {
      "type": "framework"
    }],
    ["build-plugin-fusion", {
      "disableModularImport": true,
      "themePackage": "@alifd/theme-design-pro",
      "themeConfig": {
        "css-prefix": "next-icestark-"
      }
    }],
    ["build-plugin-moment-locales", {
      "locales": ["zh-cn"]
    }],
    ["build-plugin-ignore-style", {
      "libraryName": "@alifd/next"
    }]
  ]
}
