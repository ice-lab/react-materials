    "glob": "^7.1.4",
    "husky": "^3.0.1",
    "ice-devtools": "^2.4.3-1",
    "ice-plugin-component": "^0.1.4",
    "ice-plugin-fusion": "^0.1.9",
    "ice-plugin-moment-locales": "^0.1.0",
    "ice-scripts": "^2.1.5",
    "request-promise": "^4.2.4",
