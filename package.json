{
  "private": true,
  "kitVersion": "3.x",
  "scripts": {
    "upload-oss": "node scripts/upload-oss.js",
    "generate": "idev generate",
    "generate:ci": "CONCURRENCY=5 LOG_LEVEL=verbose REGISTRY=https://registry.npmjs.org idev generate",
